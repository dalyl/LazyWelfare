@{
    Layout = null;
}


<div class="row" role="main">
    <div class="col-lg-9">
        <form role="form">
            <fieldset>
                <legend class="valtype" data-valtype="text">服务信息</legend>
                <input type="hidden" name="Domain" value="" />
                <div class="form-group">
                    <label class="control-label">服务名称</label>
                    <div class="controls">
                        <input name="Name" type="text" placeholder="名称" class="form-control" value="">
                        <p class="help-block">服务速记名称</p>
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label">服务地址</label>
                    <div class="controls">
                        <input name="Address" type="text" placeholder="地址" class="form-control" value="">
                        <p class="help-block">http://*****</p>
                    </div>
                </div>

                <div class="form-group">
                    <!-- Button -->
                    <div class="controls">
                        <button class="btn btn-success HostModelSave">保存</button>
                    </div>
                </div>
            </fieldset>
        </form>
    </div>
</div>



 